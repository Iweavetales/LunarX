{
  "name": "lunargate-test-helloworld3",
  "version": "0.1.46",
  "description": "The LunarGate that is flexible & improved SSR Framework more than Next.JS and Remix.Run.",
  "main": "index.js",
  "scripts": {
    "clean-lunar-dist": "npm run clean",
    "clean":  "rm -rf ./dist/",
    "compile": "tsc -p tsconfig.json",
    "build": "npm run release",
    "prepublishOnly": "npm run clean && cd ../../ && turbo run build",
    "escape-lightningcss-error": "CSS_TRANSFORMER_WASM=false",
    "release": "taskr release",
    "build:builder": "npm run escape-lightningcss-error &&  esbuild ApplicationBuilder/src/index.ts --bundle --platform=node --outfile=ApplicationBuilder/dist/builder.js --external:esbuild   --sourcemap=external   --external:@swc --loader:.node=file",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:builder:comment": " # Application Builder 를 빌드",
    "build:app:dev:watch": "run-p build:app:dev:watch:*",
    "build:app:dev:watch:app": "NODE_ENV=development npm run build:builder && env NODE_ENV=development node packages/LunarGate/ApplicationBuilder/dist/builder.js --enable-source-maps",
    "build:app:dev:watch:comment": "# Application Builder 를 빌드하고 앱을 증분 빌드 시작",
    "build:app:prd": "npm run build:builder && env NODE_ENV=production node packages/LunarGate/ApplicationBuilder/dist/builder.js --enable-source-maps",
    "deno-runtime-fmt": "deno fmt ./packages/LunarGate/denoRuntime/",
    "deno-runtime-lint": "deno lint ./packages/LunarGate/denoRuntime/",
    "deno-runtime": "NODE_ENV=development deno run --allow-all packages/LunarGate/denoRuntime/index.ts ",
    "build:deno-runtime": "mkdir -p ./dist/denoRuntime/ && deno bundle ./denoRuntime/index.ts ./dist/denoRuntime/index.js"
  },
  "author": "Iweavetales",
  "license": "ISC",
  "bin": {
    "lunar": "./dist/bin/lunar"
  },
  "files": [
    "dist",
    "config.d.ts",
    "document.d.ts",
    "document.js",
    "head.d.ts",
    "head.js,",
    "lunarApp.d.ts",
    "lunarApp.js",
    "lunarContext.d.ts",
    "lunarContext.js,",
    "router.d.ts",
    "router.js",
    "serverFetches.d.ts",
    "serverFetches.js"
  ],
  "dependencies": {
    "esbuild": "^0.19.1",
    "swc": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/plugin-transform-modules-amd": "^7.20.11",
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/types": "^7.18.9",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.40",
    "@swc/plugin-styled-components": "^1.5.47",
    "@taskr/typescript": "^1.1.0",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^18.7.16",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/typescript": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vanilla-extract/esbuild-plugin": "^2.2.2",
    "autoprefixer": "^10.4.13",
    "babel-plugin-styled-components": "^2.0.7",
    "chalk": "^5.0.1",
    "css-tree": "^2.2.1",
    "esbuild": "^0.19.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "javascript-obfuscator": "^4.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2",
    "node-watch": "^0.7.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "queue": "^6.0.2",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.2",
    "taskr": "^1.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^5.1.6",
    "util": "^0.12.4",
    "yaml": "^2.1.1"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
